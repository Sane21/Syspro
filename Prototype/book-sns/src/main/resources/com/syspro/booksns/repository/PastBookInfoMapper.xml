<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.syspro.booksns.repository.PastBookInfoMapper">
	<select id="selectAll" resultType="PastBookInfo">
		SELECT * FROM pastBookInfo
	</select>
	
	<select id="selectByPrimaryKey" resultType="PastBookInfo">
		SELECT * FROM pastBookInfo
		WHERE past_book_info_id = #{pastBookInfoId}
	</select>
	
	<insert id="insert">
		INSERT INTO pastBookInfo(book_info_id, last_date)
		VALUES(#{bookInfoId}, #{lastDate})
	</insert>
	
	<update id="updateByPrimaryKey">
		UPDATE pastBookInfo
		SET book_info_id = #{bookInfoId}, last_date = #{lastDate}
		WHERE past_book_info_id = #{pastBookInfoId}
	</update>
	
	<delete id="deleteByPrimaryKey">
		DELETE FROM pastBookInfo
		WHERE past_book_info_id = #{pastBookInfoId}
	</delete>
</mapper>